{
  "source": {
    "include": [
      "js-css-animations"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|doc)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./jsdoc/doc",
    "template": "./jsdoc/template"
  }
}